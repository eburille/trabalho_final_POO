public class TV extends DispositivoInteligente {
    private int canal;
    private int volume;
    
    public TV(String nome) {
        super(dispositivo);
        this.canal = 1;
        this.volume = 50;
    }
    
    public int getCanal() {
        return canal;
    }
    
    public void setCanal(int canal) {
        this.canal = canal;
    }
    
    public int getVolume() {
        return volume;
    }
    
    public void aumentarVolume() {
        if (volume < 100) {
            volume++;
        }
    }
    
    public void diminuirVolume() {
        if (volume > 0) {
            volume--;
        }
    }
}
